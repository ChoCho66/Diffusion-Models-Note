{
  "python.analysis.typeCheckingMode": "basic",
  "grammarly.selectors": [
    {
      "language": "markdown,quarto,rmarkdown",
      "pattern": "**/*.qmd,**/*.txt,*.qmd,*.txt,*.md,*.rmd"
    },
    {
      "language": "quarto",
      "scheme": "file",
      "pattern": "Note/Note.qmd"
    },
    {
      "language": "quarto",
      "scheme": "file",
      "pattern": "_Appendix.qmd"
    },
    {
      "language": "quarto",
      "scheme": "file",
      "pattern": "_Experiments-Conclusion.qmd"
    },
    {
      "language": "quarto",
      "scheme": "file",
      "pattern": "index.qmd"
    }
  ]
}